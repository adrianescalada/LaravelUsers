{
    "name": "wdna/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://gitlab.com/wdna/users",

    
    "authors": [
        {
            "name": "Adrian Escalada",
            "email": "adrianescalada@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.3.0",
        "illuminate/database": "^5.3.0",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "wdna\\ApiGuard\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
